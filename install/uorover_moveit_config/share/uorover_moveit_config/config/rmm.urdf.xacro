<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="rmm">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <xacro:include filename="$(find uorover_arm)/urdf/rmm.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="rmm_ros2_control.xacro" />


    <xacro:rmm_ros2_control name="ArmSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
